.ruler
    border-bottom: 1px solid #cecece
    &__wrapper
        padding: 35px 0
    &__list
        display: flex
        justify-content: space-between
        align-items: center
    &__item
        flex: 0 1 205px
        display: flex
        align-items: center
        div
            margin-left: 5px
        span
            display: block
            font-size: 13px
            color: $cor02
            line-height: 15px
        &.frete
            &:before
                content: '';
                display: inline-block;
                width: 35px
                height:35px
                margin-left: 0.31rem;
                background-image: url('data:image/png;base64,');
                background-repeat: no-repeat;
                background-position: center;
                background-size: 35px
        &.card
            &:before
                content: '';
                display: inline-block;
                width: 35px
                height:35px
                margin-left: 0.31rem;
                background-image: url('data:image/png;base64,');
                background-repeat: no-repeat;
                background-position: center;
                background-size: 35px
        &.site
            &:before
