.header
    position: fixed
    top: 0
    left: 0
    width: 100%
    z-index: 99
    display: flex
    flex-wrap: wrap
    &__topbar
        background-color: $cor02
        width: 100%
        transition: all .3s
        
        @media(max-width : 991px)
            order: 1
            display none
        &-container
            max-width: 1190px
            width: 100%
            margin: 0 auto
            display: flex
            align-items: center
            justify-content: space-between
            min-height 35px

            ul
                display flex
                align-items: center

                li
                    margin 0 3px

                span
                    font-size: 13px
                    color: #fffbfc
                    margin-right: 10px
                    text-transform: uppercase
            ul.telefone__mid 
                li 
                    color: #fff
                    font-size: 13px
                    font-weight: 600
                    display: flex
                    align-items: center

                    img
                        margin-right 5px

                    &:last-child
                        &:before
                            content: '|'
                            margin 0 10px 0 5px
                            font-weight 300

            .links__topbar
                li
                    &:last-child
                        &:before
                            content: '|'
                            margin 0 10px 0 5px
                            font-weight 300
                            color: #fff
                    a
                        font-size 13px
                        color: #fff
                        text-transform: uppercase

        &-list
            padding-left: 220px
            display: flex
            align-items: center
            justify-content: space-between
            @media(max-width : 1249px)
                max-width: 55%
                padding: 0
            @media(max-width : 991px)
                max-width: 100%
                padding: 0 30px
        .slick-prev
            left 0
            width: 30px
            height 30px
        .slick-next
            right 0
            width: 30px
            height 30px
        &-item
            padding-right: 35px
            font-size: 12px
            line-height: 15px
            font-weight: 800
            color: #FFFFFF
            display: flex!important
            align-items: center
            @media(max-width : 1300px)
                padding-right: 15px
            @media(max-width : 1249px)
                justify-content: center
            @media(max-width : 991px)
                padding: 0
            &--two
                @media(max-width : 991px)
                    font-size: 0px
                    &:after
                        content: '25 anos no mercado'
                        font-size: 12px

        &-phones
            display: flex
            align-items: center
            justify-content: space-between
            @media(max-width : 991px)
                display: none
        &-phone
            font-size: 12px
            color: #FFFFFF
            line-height: 15px
            display: flex
            align-items: center

            @media(max-width 991px)
                color: #333
                font-weight: 600
            &--one
                padding-right: 20px
    &__mainbar
        background: #fff;
        position: relative
        z-index: 1
        width: 100%
        border-bottom: 1px solid #cecece
        z-index: 999
        
 
        &-container
            max-width: 1190px
            width: 100%
            margin: 0 auto
            
        &-wrapper
            display: flex
            align-items: center
            justify-content: space-between
            min-height: 100px
            padding: 19px 0
            @media(max-width : 991px)
                flex-wrap: wrap

                .menu   
                    display none!important
        &-mobile
            display: none
            @media(max-width : 991px)
                display: block
                width: 27px
                height 22px


        &-logo
            position: relative
            display: flex
            align-items: center

            .headerLogo
                max-width: 264px
                width: 264px
                img 
                    max-width: 264px

            @media(max-width : 991px)
                width 33%
                justify-content center

                a
                    width 100%

        &-serach
            max-width: 360px
            width: 100%
            position relative
            @media(max-width : 991px)

                max-width: 100%
                order 1
                padding-bottom: 11px
                padding-top: 31px
                transition: all .3s
            .headerSearch
                width: 100%
                float: none
                margin: 0
                opacity: 1
                visibility: visible

            input.inputSearch 
                font-size: 13px
                color: #666666 
                border: 1px solid #d9d9d9 !important;
                border-radius 50px
                &::placeholder
                    font-size: 13px
                    color: #666666

            button#btnBusca.btSearch
                position absolute
                background-color: $cor01
                background-image: url('data:image/svg+xml;base64,')
                background-repeat: no-repeat
                background-position: center
                background-size: 20px
                border-radius 50px
                height 39px
                width: 60px
                &:hover
                    color: #ffffff
                .icon.isearch
                    display: none
                    
        &-account, &-order, &-contact
            border-right: 1px solid #D8D8D8
            padding: 12px 20px
            @media(max-width : 991px)
                display: none
            a
                color: #373535
                font-size: 12px
                text-transform: uppercase
                font-weight: 600
                white-space: nowrap
                border-bottom: 1px solid transparent
                &:hover
                    border-bottom: 1px solid #fff115

        &-right
            display: flex
            align-items: center
            justify-content: space-between
            max-width: 480px
            width 100%
            position relative

            @media(max-width : 991px)
                width auto

        &-links
            display: flex 
            align-items: center

            img
                margin-right 10px
                max-width: 25px;
            
            span
                font-size 13px
                font-weight: 600
                color: #333
                display: block

                a
                    display block
                    color: #333
                    font-weight: 800

                   
                        
            &.atendimento
                position relative
                min-height: 50px

                > span 

                    > a 
                        &:after 
                            content: '';
                            display: inline-block;
                            width: 10px;
                            height: 10px;
                            margin-left: 5px;
                            background-image: url('data:image/svg+xml;base64,');
                            background-repeat: no-repeat;
                            background-position: center;
                            background-size: 10px;

                .online
                    .email
                        color: $cor01
                        font-weight: 800

                    .tel
                        font-weight 800
                .fisico
                    margin-top: 20px 
                    padding-top 20px
                    border-top: 1px solid #cecece
                    .loja
                        text-transform uppercase
                    .tel
                        font-weight 800

                .horario
                    display: flex 
                    justify-content: flex-start
                    &:before
                        content: '';
                        display: inline-block;
                        min-width: 12px
                        width: 12px;
                        height: 12px;
                        margin-right: 5px;
                        background-image url('data:image/svg+xml;base64,')
                        background-repeat: no-repeat;
                        background-position: center;
                        background-size: 12px;
                        margin-top: 4px;
                        
                .tel
                    display: flex 
                    justify-content: flex-start
                    &:before
                        content: '';
                        display: inline-block;
                        min-width: 12px
                        width: 12px;
                        height: 12px;
                        margin-right: 5px;
                        background-image url('data:image/svg+xml;base64,')
                        background-repeat: no-repeat;
                        background-position: center;
                        background-size: 12px;
                        margin-top: 4px;

                
                .whatsapp
                    display: flex 
                    justify-content: flex-start
                    
                    &:before
                        content: '';
                        display: inline-block;
                        min-width: 12px
                        width: 12px;
                        height: 12px;
                        margin-right: 5px;
                        background-image: url('data:image/png;base64,')

                        background-repeat: no-repeat;
                        background-position: center;
                        background-size: 12px;
                        margin-top: 4px;

                

                .email
                    display: flex 
                    justify-content: flex-start
                    &:before
                        content: '';
                        display: inline-block;
                        min-width: 12px
                        width: 12px;
                        height: 12px;
                        margin-right: 5px;
                        background-image url('data:image/svg+xml;base64,')
                        background-repeat: no-repeat;
                        background-position: center;
                        background-size: 12px;
                        margin-top: 4px;

                .endereco
                    display: flex 
                    justify-content: flex-start
                    &:before
                        content: '';
                        display: inline-block;
                        min-width: 12px
                        width: 12px;
                        height: 12px;
                        margin-right: 5px;
                        background-image url('data:image/svg+xml;base64,')
                        background-repeat: no-repeat;
                        background-position: center;
                        background-size: 12px;
                        margin-top: 4px;

                .atendimento__sub
                    display: none;
                    position: absolute
                    top: 100%
                    left: 50%
                    transform: translateX(-50%)
                    padding: 25px 20px
                    background-color: #ffffff
                    border: 1px solid #d2d2d2
                    transition: 1s
                    cursor: auto

                    h3
                        font-size: 15px
                        text-transform: uppercase
                        font-weight: 800
                        margin-bottom: 10px
                        color: $cor02
                        &:before
                            content: ''
                            width: 100%
                            height: 15px 
                            position absolute
                            top: 0
                            left: 0
                            background-color: #fff
                    &:before
                        content: ''
                        width: 15px 
                        height: 15px
                        transform: rotate(45deg)
                        position: absolute
                        top: -8px
                        left: 47%
                        background-color: #d2d2d2
                    &:after
                        content: ''
                        width: 100% 
                        height: 15px
                        position: absolute
                        top: -15px
                        left: 0
                        background-color: transparent
                &:hover
                    .atendimento__sub
                        display: block
            &.user-bemvindo
                .login__sub
                    display: none;
                    position: absolute
                    top: 50px
                    left: 50%
                    transform: translateX(-50%)
                    padding: 20px 15px
                    background-color: #ffffff
                    border: 1px solid #d2d2d2
                    transition: 1s
                    h3
                        font-size: 15px
                        text-transform: uppercase
                        font-weight: 800
                        margin-bottom: 10px
                        color: $cor02
                        &:before
                            content: ''
                            width: 100%
                            height: 15px 
                            position absolute
                            top: 0
                            left: 0
                            background-color: #fff
                    a
                        text-transform uppercase
                        &:after
                            display: none
                    > a
                        &:first-child
                            &:before
                                content: '';
                                width: 100%;
                                height: 0.94rem;
                                position: absolute;
                                top: -0.94rem;
                                left: 0;
                                background-color: transparent;
                    &:before
                        content: ''
                        width: 15px 
                        height: 15px
                        transform: rotate(45deg)
                        position: absolute
                        top: -8px
                        left: 47%
                        background-color: #d2d2d2
                    &:after
                        content: ''
                        width: 100% 
                        height: 15px
                        position: absolute
                        top: 0
                        left: 0
                        background-color: #fff
                &:hover
                    .login__sub
                        display: block
        &-cart
            padding-left: 10px
            display: flex 
            span
                font-size: 13px;
                font-weight: 600;
                color: #333;
                margin-left: 5px;
            @media(max-width : 991px)
                padding: 0
            .carrinho.carrinhoHead
                a
                    float none
                    display: flex!important
                    align-items: center
                .icon.icart
                    display: none
                .minicart-txt-itens
                    display: none
                .minicart-qtde-itens
                    color: $cor02
                    font-size: 13px
                    color: #333;
                    font-weight: 800;
                    float: none
                    position: absolute
                    left: 25px
                    bottom: -12px
                    &:before
                        content: '';
                        display: inline-block;
                        width: 10px;
                        height: 10px;
                        margin-left: 5px;
                        background-image: url('data:image/svg+xml;base64,');
                        background-repeat: no-repeat;
                        background-position: center;
                        background-size: 10px;
                        position: absolute;
                        bottom: 10px;
                        right: -15px;
                    &:after
                        content: 'Item'
                        margin-left: 5px

    @media(max-width: 1290px)
        &__topbar-container
        &__mainbar-container
            max-width: 991px
        &__mainbar 
            &-links
                min-width: 113px
                margin-right 20px
                img 
                    display none
            &-cart
                span 
                    display none
                span.minicart-qtde-itens
                    display block
                    background-color transparent 
                    left: 10px !important
                    &:before
                    &:after
                        display none !important
                .carrinho.carrinhoHead
                    position relative
                    a
                        margin-right: 10px;
                span.minicart-qtde-itens
                    left: 10px;
            &-right
                max-width: 315px;
    @media(max-width: 991px)
        &__mainbar
            &-container
                padding 0 15px
            &-links
                display none
            &-logo
                width: 60%
                .headerLogo
                    max-width: 100% !important 
                    img 
                        max-width: 100% !important
            &-mobile
                height: 30px
            &-serach
                padding-top 10px

        

    